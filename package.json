    "copy-webpack-plugin": "^6.0.3",
    "cross-env": "^5.0.5",
    "css-loader": "^3.6.0",
    "file-loader": "^1.1.4",
    "flagpack-core": "^2.0.0",
    "node-sass": "^4.5.3",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.0.0",
    "sass-loader": "^6.0.6",
    "vue": "2.5.11",
    "vue-loader": "13.0.5",
    "vue-template-compiler": "2.5.11",
    "webpack": "^4.43.0",
    "webpack-dev-server": "^3.11.0"
  },
