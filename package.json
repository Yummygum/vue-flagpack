{
  "name": "vue-flagpack",
  "description": "Gorgeous flag components for your Vue project.",
  "version": "1.0.1",
  "author": "Yummygum <info@yummygum.com> (https://yummygum.com)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yummygum/vue-flagpack.git"
  },
  "keywords": [
    "vue",
    "vue-component",
    "languages",
    "flag",
    "iso",
    "flag set",
    "countries"
  ],
  "files": [
    "dist/*"
  ],
  "main": "./dist/src/main.js",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Yummygum/vue-flagpack/issues"
  },
  "homepage": "https://flagpack.xyz",
  "scripts": {
    "dev": "cross-env NODE_ENV=development rollup -c -w",
    "prebuild": "rm -rf dist",
    "generate-flags": "node ./src/generate-flags.js",
    "build": "cross-env NODE_ENV=production rollup -c && npm run generate-flags"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
